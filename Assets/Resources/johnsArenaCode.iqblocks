.0 * -1, degrees, true);\n  Drivetrain.driveFor(400.0, mm, true);\n  Drivetrain.turnFor(90.0, degrees, true);\n  Drivetrain.driveFor(900.0, mm, true);\n  Drivetrain.driveFor(200.0 * -1, mm, true);\n  Drivetrain.turnFor(90.0, degrees, true);\n  Drivetrain.driveFor(200.0 * -1, mm, true);\n  Drivetrain.driveFor(200.0, mm, true);\n  Drivetrain.turnFor(90.0, degrees, true);\n  Drivetrain.driveFor(400.0, mm, true);\n  Drivetrain.turnFor(90.0, degrees, true);\n  Drivetrain.driveFor(300.0, mm, true);\n  Drivetrain.driveFor(300.0 * -1, mm, true);\n  Drivetrain.turnFor(90.0 * -1, degrees, true);\n  Drivetrain.driveFor(700.0, mm, true);\n  Drivetrain.turnFor(90.0, degrees, true);\n  Drivetrain.driveFor(400.0, mm, true);\n  Drivetrain.turnFor(180.0, degrees, true);\n  Drivetrain.driveFor(800.0, mm, true);\n  Drivetrain.turnFor(90.0 * -1, degrees, true);\n  Drivetrain.driveFor(200.0 * -1, mm, true);\nreturn 0;\n}\n\n\nint main() {\n\n\n  srand(vex::timer::system());\n\n  // calibrate the drivetrain gyro\n  task::sleep(200);\n  Brain.Screen.print(\"Calibrating Gyro\");\n  while (TurnGyroSmart.isCalibrating()) {\n    task::sleep(25);\n  }\n  Brain.Screen.clearScreen();\n  Brain.Screen.setCursor(1,1);\n\n  // pre event registration\n  // register event handlers\n\n  task::sleep(15);\n  // post event registration\n\n  // when started blocks...\n\n  whenStarted1();\n  return 0;\n}","cppStatus":"true","platform":"IQ","sdkVersion":"20200120.14.00.00","appVersion":"1.0.9-45","fileFormat":"0.1.0","icon":""}